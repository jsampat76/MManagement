<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.bluesystech.iCorpMM" version="2.0.0">
    <name>iCorpMM</name>
    <description>Meeting Schedular for high designated Corporates.</description>
    <author href="http://bluesys.in" email="jmayuri86@gmail.com">Mayuri Dekhane</author>
    <preference name="phonegap-version" value="cli-6.5.0" />
    
  <icon src="icon.png" />
  <splash src="splash.png" />
